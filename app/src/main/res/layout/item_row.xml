<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    >

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:padding="6dp"
        android:layout_marginTop="5dp"
        android:layout_alignParentEnd="false"
        android:layout_alignParentStart="false"
        android:layout_alignParentTop="false"
        android:layout_alignParentLeft="false"
        android:layout_alignParentBottom="false"
        android:layout_alignParentRight="false"
        android:layout_alignWithParentIfMissing="false"
        android:id="@+id/childLayout"

        android:background="#6e000000">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Large Text"
            android:id="@+id/NaslovVezbe"
            android:background="#7fff7600"
            android:textColor="#ffffffff" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|start"
            android:id="@+id/linearLayout2"
            android:layout_marginTop="3dp">

            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="1st Set  15 Reps"
                android:id="@+id/textView10"
                android:textSize="18dp"
                android:textColor="#ffffffff"
                android:layout_gravity="center" />

            <TextView
                android:layout_width="35dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="32.5"
                android:id="@+id/textViewSet1"
                android:textSize="18dp"
                android:layout_marginLeft="10dp"
                android:textColor="#ff7600"
                android:layout_gravity="center" />

            <EditText
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:ems="10"
                android:id="@+id/editTextSet1"
                android:layout_marginLeft="15dp"
                android:text="0"
                android:textColor="#ffffff"
                android:selectAllOnFocus="true"
                android:theme="@style/MyEditTextTheme" />

            <LinearLayout
                android:id="@+id/linearLayoutBtn1"
                android:layout_width="81dp"
                android:layout_height="40dp"
                android:background="@drawable/new_button2"
                android:clickable="true"
                android:orientation="horizontal"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:focusable="false"
                android:layout_gravity="center"
                android:layout_marginLeft="15dp">

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp" />

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="DONE"
                    android:textSize="16dp"
                    android:textColor="#ffffffff"
                    android:layout_marginRight="4dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|start"
            android:id="@+id/linearLayout3"
            android:weightSum="1"
            android:layout_marginTop="3dp">

            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="2nd Set  12 Reps"
                android:id="@+id/textView12"
                android:textSize="18dp"
                android:textColor="#ffffffff" />

            <TextView
                android:layout_width="35dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="32.5"
                android:id="@+id/textViewSet2"
                android:textSize="18dp"
                android:layout_marginLeft="10dp"
                android:textColor="#ff7600"
                android:textIsSelectable="false" />

            <EditText
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:ems="10"
                android:id="@+id/editTextSet2"
                android:layout_marginLeft="15dp"
                android:text="0"
                android:textColor="#ffffff"
                android:selectAllOnFocus="true"
                android:theme="@style/MyEditTextTheme" />

            <LinearLayout
                android:id="@+id/linearLayoutBtn2"
                android:layout_width="81dp"
                android:layout_height="40dp"
                android:background="@drawable/new_button2"
                android:clickable="true"
                android:orientation="horizontal"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:layout_gravity="center"
                android:layout_marginLeft="16dp" >

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp" />

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="DONE"
                    android:textSize="16dp"
                    android:textColor="#ffffffff"
                    android:layout_marginRight="4dp" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|start"
            android:id="@+id/linearLayout4"
            android:layout_marginTop="3dp">

            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="3rd Set  10 Reps"
                android:id="@+id/textView14"
                android:textSize="18dp"
                android:textColor="#ffffffff" />

            <TextView
                android:layout_width="35dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="32.5"
                android:id="@+id/textViewSet3"
                android:textSize="18dp"
                android:layout_marginLeft="10dp"
                android:textColor="#ff7600" />

            <EditText
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:ems="10"
                android:id="@+id/editTextSet3"
                android:layout_marginLeft="14dp"
                android:text="0"
                android:textColor="#ffffff"
                android:selectAllOnFocus="true"
                android:theme="@style/MyEditTextTheme" />

            <LinearLayout
                android:id="@+id/linearLayoutBtn3"
                android:layout_width="81dp"
                android:layout_height="40dp"
                android:background="@drawable/new_button2"
                android:clickable="true"
                android:orientation="horizontal"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:layout_gravity="center"
                android:selectAllOnFocus="true"
                android:layout_marginLeft="15dp" >

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp" />

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="DONE"
                    android:textSize="16dp"
                    android:textColor="#ffffffff"
                    android:layout_marginRight="4dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|start"
            android:id="@+id/linearLayout5"
            android:layout_marginTop="3dp">

            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="4th Set   8 Reps"
                android:id="@+id/textView16"
                android:textSize="18dp"
                android:textColor="#ffffffff" />

            <TextView
                android:layout_width="35dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="32.5"
                android:id="@+id/textViewSet4"
                android:textSize="18dp"
                android:layout_marginLeft="10dp"
                android:textColor="#ff7600" />

            <EditText
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:ems="10"
                android:id="@+id/editTextSet4"
                android:layout_marginLeft="15dp"
                android:text="0"
                android:textColor="#ffffff"
                android:selectAllOnFocus="true"
                android:theme="@style/MyEditTextTheme" />

            <LinearLayout
                android:id="@+id/linearLayoutBtn4"
                android:layout_width="81dp"
                android:layout_height="40dp"
                android:background="@drawable/new_button2"
                android:clickable="true"
                android:orientation="horizontal"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:layout_gravity="center"
                android:layout_marginLeft="15dp" >

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp" />

                <TextView
                    android:id="@+id/textView21"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="DONE"
                    android:textSize="16dp"
                    android:textColor="#ffffffff"
                    android:layout_marginRight="4dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutBtnComplited"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:background="@drawable/new_button2"
            android:clickable="true"
            android:orientation="horizontal"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:focusable="false"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10dp" >

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="5dp" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_margin="5dp"
                android:text="COMPLETED"
                android:textSize="22dp"
                android:textColor="#ffffffff" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="hidden"
            android:id="@+id/hiddent"
            android:layout_gravity="right"
            android:visibility="invisible" />

    </LinearLayout>


</RelativeLayout>
